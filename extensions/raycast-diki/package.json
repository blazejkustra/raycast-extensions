{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-diki",
  "title": "Diki Translate",
  "description": "Diki Translate with autosuggestions",
  "icon": "command-icon.png",
  "author": "blazej_kustra",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Diki Translate",
      "description": "Diki Translate with autosuggestions",
      "mode": "view",
      "keywords": [
        "diki",
        "translate",
        "suggestions",
        "autocomplete"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3",
    "node-fetch": "^3.1.0",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
